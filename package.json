{
  "name": "winenotes",
  "sideEffects": false,
  "version": "0.4.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "export NODE_ENV=development && webpack-dev-server --open --config webpack.dev.js",
    "build": "export NODE_ENV=production && webpack --config webpack.prod.js",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "test": "export NODE_ENV=testing && jest",
    "precommit": "lint-staged",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint src --fix --ext .js",
      "git add"
    ]
  },
  "homepage": "https://mikelbrownus.github.io/WineNotes",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.0.07",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.0.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.11",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^4.1.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.1.2",
    "autosuggest-highlight": "^3.1.1",
    "localforage": "^1.7.3",
    "ramda": "^0.26.1",
    "react": "^16.5.1",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.5.1",
    "react-icons": "^3.2.2",
    "react-router-dom": "^4.3.1",
    "uuid": "^3.3.2"
  }
}
